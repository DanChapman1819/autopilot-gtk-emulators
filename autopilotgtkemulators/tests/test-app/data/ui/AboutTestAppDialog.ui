<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <!-- interface-requires about_test_app_dialog 1.0 -->
  <object class="AboutTestAppDialog" id="about_test_app_dialog">
    <property name="can_focus">False</property>
    <property name="border_width">5</property>
    <property name="icon">../media/test-app.svg</property>
    <property name="type_hint">normal</property>
    <property name="program_name">Test App</property>
    <property name="version">0.1</property>
    <property name="copyright" translatable="yes">Copyright 2013, Dan Chapman.</property>
    <property name="comments" translatable="yes">This is a test app for autopilot gtk emulator toolkit.

It basically checks the emulator is an instance of the widget then runs any defined utility functions against the widget.</property>
    <property name="website">https://launchpad.net/autopilot-gtk-emulator</property>
    <property name="website_label" translatable="yes">autopilot-gtk-emulators</property>
    <property name="authors">Dan Chapman</property>
    <property name="logo">../media/test-app.svg</property>
    <property name="license_type">custom</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="AboutTestAppDialog-vbox1">
        <property name="can_focus">False</property>
        <child internal-child="action_area">
          <object class="GtkButtonBox" id="AboutTestAppDialog-action_area1">
            <property name="can_focus">False</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
